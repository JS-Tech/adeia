<%= form_for @token do |f| %>

  <%= f.label :adeia_permission_id %>
  <%= f.collection_select :adeia_permission_id, Adeia::Permission.all, :id, :full_name %>

  <%= f.label :exp_at %>
  <%= f.date_select :exp_at %>

  <%= f.label :is_valid %>
  <%= f.check_box :is_valid %>

  <%= f.submit %>

<% end %>